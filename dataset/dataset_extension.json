{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://www.w3id.org/biocompute/extension_domain/1.0.0/dataset/dataset_extension.json",
    "type": "object",
    "required": [
        "category_value",
        "category_name"
    ],
    "properties": {
        "category_value": {
            "type": "string",
            "examples": [
                "Homo sapiens"
            ]
        },
        "category_name": {
            "type": "string",
            "examples": [
                "species"
            ]
        }
    }
}
