{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://www.w3id.org/biocompute/1.3.0/schemas/extension_domain/scm_extension.json",
    "type": "object",
    "required": [
        "data_license",
        "scripts_license"
    ],
    "properties": {
        "data_license": {
            "type": "string",
            "examples": [
                "https://github.com/example/repo1"
            ],
            "format": "uri"
        },
        "scripts_license": {
            "type": "string",
            "examples": [
                "https://github.com/example/repo1"
            ],
            "format": "uri"
        }
    }
}
